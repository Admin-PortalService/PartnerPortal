                            <div class="dropdown">
                                <button class="dropbtn">Master</button>
                                <div class="dropdown-content">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Projects" asp-action="Index">Project</a>
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Module" asp-action="Index">Module</a>
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Users" asp-action="Create">User</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item">
                           <div class="dropdown">
