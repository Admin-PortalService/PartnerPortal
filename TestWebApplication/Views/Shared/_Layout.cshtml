                                    <a class="nav-link text-dark" asp-area="" asp-controller="Incident" asp-action="IndexIncident">View Tickets</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Incident" asp-action="Create">Request</a>
                            </div>
                           </div>
                        </li>
                       

                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
